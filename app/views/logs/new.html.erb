<div class="row">
  <div class="col-md-8 ml-auto mr-auto px-5">
    <div class="card card-transparent">
      <div class="card-header card-header-rose text-center">
        <h3 class="mt-0 mb-1"><i class="icons8-<%= @activity.icon %>" style="font-size:5rem"></i><br />New <%= @activity.name.downcase %> session</h3>
      </div>
      <div class="card-body px-5 pb-4">
        <div class="text-center">
          <%= render 'form', log: @log%>
        </div>

        <%= link_to 'Back', logs_path %>
      </div>
    </div>
  </div>
</div>